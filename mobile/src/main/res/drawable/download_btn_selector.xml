<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <solid android:color="@color/classic_download_end"/>
            <corners android:radius="3dp"/>
        </shape>
    </item>
    <item>
        <shape android:shape="rectangle">
            <gradient
                android:angle="270"
                android:endColor="@color/classic_download_end"
                android:startColor="@color/classic_download_start"/>
            <corners android:radius="3dp"/>
        </shape>
    </item>
</selector>